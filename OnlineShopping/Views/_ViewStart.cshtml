@{
    if(Request.IsAuthenticated)
    {
        if(User.IsInRole("Admin"))
        {
            Layout = "~/Views/AdminShared/_LayoutAdmin.cshtml";
        }
        else
        {
            Layout = "~/Views/Shared/_Layout.cshtml";
        }
    }
    else
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

}
